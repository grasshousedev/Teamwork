{
	"manifest_version": 2,
	"name": "WorkPlay Pomodoro",
	"version": "0.1",
	"browser_action": {
		"default_title": "WorkPlay Pomodoro",
		"default_popup": "views/popup.html"
	}, 
	"background": {
		"persistent": false,
		"scripts": ["src/js/helpers.js", "spec/mocks.js",
			"src/js/Task.js", "src/js/TaskRepository.js", 
			"src/js/TaskService.js", "src/js/background.js"]
	},
	"permissions": ["storage"]

}